<?xml version="1.0" encoding="utf-8"?>
<resources>


    <attr name="colorAll" format="color"></attr>
    <attr name="colorSecond" format="color"></attr>
    <attr name="circleWidth" format="dimension"></attr>
    <attr name="firstColor" format="color"></attr>
    <attr name="secondColor" format="color"></attr>

    <declare-styleable name="MyLoadingView">
        <attr name="colorAll"></attr>
        <attr name="colorSecond"></attr>
        <attr name="lineWidth" format="dimension"></attr>
        <attr name="lineHeight" format="dimension"></attr>
        <attr name="mBackground" format="color"></attr>
    </declare-styleable>


    <declare-styleable name="MyProgressBar">
        <attr name="firstColor"></attr>
        <attr name="secondColor"></attr>
        <attr name="mSpeed" format="integer"></attr>
        <attr name="circleWidth"></attr>
    </declare-styleable>


    <declare-styleable name="MySoundView">
        <attr name="firstColor"></attr>
        <attr name="secondColor"></attr>
        <attr name="circleWidth"></attr>
        <attr name="count" format="integer"></attr>
    </declare-styleable>

</resources>